package leetcode.array.problems;

/**
 * @author NIPC-43
 *
 *         Date : Feb 26, 2021 Time : 12:41:43 PM
 */
public class Pattern {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	    int a = 10, b = 20, c = 100;

	    if(a <= b || c <= b) {
	        System.out.println("hello");
	    }
	    else if(a <= b || a <= c) {
	        System.out.println("hi");
	    }
	    else {
	        System.out.println("hey");
	    }
	}

}
