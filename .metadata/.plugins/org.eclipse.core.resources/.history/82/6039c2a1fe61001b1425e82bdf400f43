package leetcode.string.problems;

import java.util.Scanner;

/**
 * @author NIPC-43
 *
 *         Date : Jan 29, 2021 Time : 11:46:37 AM
 */

class A {
	public static void sound() {
		System.out.println("Meow");
	}
}

public class B extends A {

	/**
	 * @param args
	 */

	public static void sound() {
		System.out.println("Roar");
	}

	public static final String pig = "length: 10";
	public static final String dog  = "length:" + pig.length();

	public static void main(String[] args) {System.out.println(pig == dog);}

}
