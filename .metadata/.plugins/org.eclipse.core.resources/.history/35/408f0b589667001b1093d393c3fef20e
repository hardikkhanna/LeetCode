package leetcode.weeklycontest.problems;

import java.util.Scanner;

/**
 * @author NIPC-43
 *
 *         Date : Feb 5, 2021 Time : 3:10:34 PM
 */
public class samlpe {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		for (int i = 10; i >= 1; i--) {
			if (n % i == 0) {
				System.out.print(i);
				break;
			}
		}
	}
}
