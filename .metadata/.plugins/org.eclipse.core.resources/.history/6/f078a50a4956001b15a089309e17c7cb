public class CountTripletsToFormArraysOfEqualXOR extends ScannerInput {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int[] arr = get1DArray();
		int ans = countTriplets(arr);
		System.out.println(ans);

	}

}
