/**
 * @author Hardik
 *
 *         Date : Jan 6, 2021 Time : 1:21:15 PM
 */
public class LinkedListCycle extends ScannerInput {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		String startdate = "01/01/2020";
		String endDate = "06/12/2020";
		SimpleDateFormat dateFormat = new SimpleDateFormat("MM/dd/yyyy");
		try {
			Date convertedDate = dateFormat.parse(startdate);

			dateFormat = new SimpleDateFormat("MM/dd/yyyy");
			Date convertedendDate = dateFormat.parse(startdate);
			/*
			 * int s = ((31 - 30) / (100 * 60 * 60 * 24)) + 1; Long start =
			 * convertedendDate.getTime(); Long end = convertedDate.getTime(); long n =
			 * (long) Math.round((start - end) / (1000 * 60 * 60 * 24));
			 */
			long enddate = 1585627200000L;
			long startdate = 1583038800000L;
			long ans = (long) ((enddate - startdate) / (double)(1000 * 60 * 60 * 24) + 1);
			
			System.out.println(1000 * 60 * 60 * 24);
			

		} catch (ParseException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}

	}

	/**
	 * @param head
	 */
	private static boolean hasCycle(ListNode head) {
		if (head == null)
			return false;
		ListNode walker = head;
		ListNode runner = head;
		while (runner.next != null && runner.next.next != null) {
			walker = walker.next;
			runner = runner.next.next;
			if (walker == runner)
				return true;
		}
		return false;

	}

}
