public class NodeWithNoSiblings extends BinaryTreeUse {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		BinaryTreeNode<Integer> root = takeInputLevelWise();
		nodeWithNoSiblings(root);
		printTreeLevelWise(root);

	}

}
