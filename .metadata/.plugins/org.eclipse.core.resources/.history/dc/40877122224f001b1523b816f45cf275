package leetcode.array.problems;

import java.util.Scanner;

/**
 * @author NIPC-43
 *
 *         Date : Jan 5, 2021 Time : 12:16:21 PM
 */
public class ScannerInput {
	Scanner sc = new Scanner(System.in);

	/**
	 * @param args
	 */
	public int[] get1DArray() {

		int n = sc.nextInt();
		int[] arr = new int[n];
		for (int i = 0; i < arr.length; i++) {
			arr[i] = sc.nextInt();
		}
		return arr;
	}

}
